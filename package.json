        "@types/jest": "^25.2.3",
        "jest-cli": "^26.0.1",
        "prettier": "^2.0.5",
        "testpack-cli": "^1.1.4",
        "ts-jest": "^26.1.1",
        "ts-node-dev": "^1.0.0-pre.49",
